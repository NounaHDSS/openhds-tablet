          <causofdeathnotdiagnosed/>
          <placeOfDeath/>
          <OtherplaceOfDeath/>
          <causeOfDeath/>
          <hh_people_nb/>
          <membershiptonewhoh_count/>
          <membershiptonewhoh jr:template="">
            <extId/>
            <memberName/>
            <socialGroupId/>
            <relationshipToGroupHead/>
          </membershiptonewhoh>
          <processedByMirth/>
          <meta>
            <instanceID/>
          </meta>
        </DEATHTOHOH>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/DEATHTOHOH/start" readonly="true()" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/DEATHTOHOH/end" readonly="true()" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/DEATHTOHOH/deviceid" readonly="true()" type="string"/>
      <bind nodeset="/DEATHTOHOH/openhds/visitId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/DEATHTOHOH/openhds/fieldWorkerId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/DEATHTOHOH/openhds/householdId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/DEATHTOHOH/openhds/individualId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/DEATHTOHOH/openhds/firstName" readonly="true()" type="string"/>
      <bind nodeset="/DEATHTOHOH/openhds/lastName" readonly="true()" type="string"/>
      <bind nodeset="/DEATHTOHOH/openhds/new_hoh_id" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/DEATHTOHOH/date" required="true()" type="date"/>
      <bind nodeset="/DEATHTOHOH/diagnoseddeath" required="true()" type="select1"/>
      <bind nodeset="/DEATHTOHOH/whom" relevant="selected( /DEATHTOHOH/diagnoseddeath , '1')" required="true()" type="select1"/>
      <bind nodeset="/DEATHTOHOH/causeofdeathdiagnosed" relevant="selected( /DEATHTOHOH/diagnoseddeath , '1')" required="true()" type="select1"/>
      <bind nodeset="/DEATHTOHOH/causofdeathnotdiagnosed" relevant="selected( /DEATHTOHOH/diagnoseddeath , '2')" required="true()" type="select1"/>
      <bind nodeset="/DEATHTOHOH/placeOfDeath" required="true()" type="select1"/>
      <bind nodeset="/DEATHTOHOH/OtherplaceOfDeath" relevant="selected( /DEATHTOHOH/placeOfDeath , 'OTHER')" required="true()" type="string"/>
      <bind calculate="concat( /DEATHTOHOH/causeofdeathdiagnosed , /DEATHTOHOH/causofdeathnotdiagnosed )" nodeset="/DEATHTOHOH/causeOfDeath" readonly="true()" type="string"/>
      <bind nodeset="/DEATHTOHOH/hh_people_nb" readonly="true()" relevant="false()" required="true()" type="int"/>
      <bind calculate=" /DEATHTOHOH/hh_people_nb " nodeset="/DEATHTOHOH/membershiptonewhoh_count" readonly="true()" type="string"/>
      <bind nodeset="/DEATHTOHOH/membershiptonewhoh/extId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/DEATHTOHOH/membershiptonewhoh/memberName" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/DEATHTOHOH/membershiptonewhoh/socialGroupId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/DEATHTOHOH/membershiptonewhoh/relationshipToGroupHead" required="true()" type="select1"/>
      <bind nodeset="/DEATHTOHOH/processedByMirth" readonly="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/DEATHTOHOH/meta/instanceID" readonly="true()" type="string"/>
    </model>
