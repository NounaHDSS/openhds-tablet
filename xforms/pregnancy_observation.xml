      <instance>
        <pregnancy_observation id="pregnancy_observation">
          <start/>
          <end/>
          <deviceid/>
          <openhds>
            <fieldWorkerId/>
            <visitId/>
            <individualId/>
            <recordedDate/>
          </openhds>
          <estimatedAgeOfPreg/>
          <pregNotes/>
          <ageOfPregFromPregNotes/>
          <anteNatalClinic/>
          <lastClinicVisitDate/>
          <healthfacility/>
          <medicineforpregnancy/>
          <ttinjection/>
          <othermedicine/>
          <pregnancyNumber/>
          <expectedDeliveryDate/>
          <processedByMirth/>
          <meta>
            <instanceID/>
          </meta>
        </pregnancy_observation>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/pregnancy_observation/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/pregnancy_observation/end" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/pregnancy_observation/deviceid" type="string"/>
      <bind nodeset="/pregnancy_observation/openhds/fieldWorkerId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/pregnancy_observation/openhds/visitId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/pregnancy_observation/openhds/individualId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/pregnancy_observation/openhds/recordedDate" readonly="true()" required="true()" type="date"/>
      <bind nodeset="/pregnancy_observation/estimatedAgeOfPreg" required="true()" type="int"/>
      <bind nodeset="/pregnancy_observation/pregNotes" required="true()" type="select1"/>
      <bind constraint=".&lt;52" nodeset="/pregnancy_observation/ageOfPregFromPregNotes" relevant="../pregNotes = 1" required="true()" type="int"/>
      <bind nodeset="/pregnancy_observation/anteNatalClinic" required="true()" type="select1"/>
      <bind constraint=".&lt; today()" nodeset="/pregnancy_observation/lastClinicVisitDate" relevant="../anteNatalClinic=1" required="true()" type="date"/>
      <bind nodeset="/pregnancy_observation/healthfacility" required="true()" type="select1"/>
      <bind nodeset="/pregnancy_observation/medicineforpregnancy" required="true()" type="select1"/>
      <bind nodeset="/pregnancy_observation/ttinjection" required="true()" type="select1"/>
      <bind nodeset="/pregnancy_observation/othermedicine" type="string"/>
      <bind nodeset="/pregnancy_observation/pregnancyNumber" required="true()" type="int"/>
      <bind constraint=".&gt; today()" nodeset="/pregnancy_observation/expectedDeliveryDate" required="true()" type="date"/>
      <bind nodeset="/pregnancy_observation/processedByMirth" readonly="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/pregnancy_observation/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
