      <instance>
        <out_migration_registration id="out_migration_registration">
          <start/>
          <end/>
          <deviceid/>
          <openhds>
            <individualId/>
            <fieldWorkerId/>
            <visitId/>
            <firstName/>
            <lastName/>
          </openhds>
          <dateOfMigration/>
          <withinRusinga/>
          <vilagezone/>
          <outofrusinga/>
          <nameOfDestination/>
          <reasonForOutMigration/>
          <maritalChange/>
          <reasonForOutMigrationOther/>
          <shortlongterm/>
          <processedByMirth/>
          <meta>
            <instanceID/>
          </meta>
        </out_migration_registration>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/out_migration_registration/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/out_migration_registration/end" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/out_migration_registration/deviceid" type="string"/>
      <bind nodeset="/out_migration_registration/openhds/individualId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/out_migration_registration/openhds/fieldWorkerId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/out_migration_registration/openhds/visitId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/out_migration_registration/openhds/firstName" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/out_migration_registration/openhds/lastName" readonly="true()" required="true()" type="string"/>
      <bind constraint="(.&lt;=today())" jr:constraintMsg="Date of migration can not be in the future or before the start of the study" nodeset="/out_migration_registration/dateOfMigration" required="true()" type="date"/>
      <bind nodeset="/out_migration_registration/withinRusinga" required="true()" type="select1"/>
      <bind nodeset="/out_migration_registration/vilagezone" relevant="../withinRusinga='1'" required="true()" type="select1"/>
      <bind nodeset="/out_migration_registration/outofrusinga" relevant="../withinRusinga='2'" required="true()" type="select1"/>
      <bind calculate="if(../withinRusinga='1', /out_migration_registration/vilagezone , /out_migration_registration/outofrusinga )" nodeset="/out_migration_registration/nameOfDestination" required="true()" type="string"/>
      <bind nodeset="/out_migration_registration/reasonForOutMigration" required="true()" type="select1"/>
      <bind nodeset="/out_migration_registration/maritalChange" relevant="../reasonForOutMigration = 'MARITAL_CHANGE'" required="true()" type="select1"/>
      <bind nodeset="/out_migration_registration/reasonForOutMigrationOther" relevant="../reasonForOutMigration = 'OTHER'" required="true()" type="string"/>
      <bind nodeset="/out_migration_registration/shortlongterm" required="true()" type="select1"/>
      <bind nodeset="/out_migration_registration/processedByMirth" readonly="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/out_migration_registration/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
